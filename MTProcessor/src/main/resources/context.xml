<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
       
    <context:component-scan base-package="com.pp.currencyfairtest.mtprocessor" />
    
    <bean id="messageSink" class="com.pp.currencyfairtest.mtprocessor.MessageMultiplexor">
        <property name="messageProcessors">
            <list>
                <ref bean="messageProcessor1" />
                <ref bean="messageProcessor2" />
            </list>
        </property>
    </bean>
    
    <bean id="messageProcessor1" class="com.pp.currencyfairtest.mtprocessor.processors.DelegatingMessageProcessor">
    </bean>
    
    <bean id="messageProcessor2" class="com.pp.currencyfairtest.mtprocessor.processors.DelegatingMessageProcessor">
    </bean>

</beans>